{
  "compilerOptions": {
    "incremental": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": ["dom", "esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "outDir": "../out/webview",
    "strict": true,
    "target": "esnext",
    "strictNullChecks": false,
    "tsBuildInfoFile": "../out/.tsbuildinfo"
  },
  "include": ["./**/*", "../src/typings/api.d.ts", "../src/typings/webview.d.ts"]
}
