{
  "name": "exercism",
  "displayName": "Exercism",
  "description": "Complete programming challenges in different languages to improve your skills.",
  "version": "1.15.1",
  "author": "Mason D. Liu",
  "publisher": "masonliu",
  "license": "MIT",
  "icon": "images/logo/logo.jpeg",
  "homepage": "https://github.com/masliu/vscode-exercism.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/masliu/vscode-exercism.git"
  },
  "bugs": {
    "url": "https://github.com/masliu/vscode-exercism/issues"
  },
  "engines": {
    "vscode": "^1.35.0"
  },
  "galleryBanner": {
    "color": "#339AA8",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "programming challenges",
    "view",
    "practice",
    "algorithms",
    "exercism",
    "problems",
    "tree",
    "questions"
  ],
  "activationEvents": [
    "onCommand:exercism.global.exportUserData",
    "onCommand:exercism.global.importUserData",
    "onView:exercism.view.tracks"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "exercism-explorer",
          "title": "Exercism",
          "icon": "images/logo/logo.svg"
        }
      ]
    },
    "views": {
      "exercism-explorer": [
        {
          "id": "exercism.view.tracks",
          "name": "Tracks"
        }
      ]
    },
    "commands": [
      {
        "command": "exercism.exercise.openStart",
        "title": "Open and Start",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.exercise.createNewFile",
        "title": "New File",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.exercise.preview",
        "title": "Open Preview",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.exercise.complete",
        "title": "Complete Exercise",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.exercise.download",
        "title": "Download Exercise",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.file.submit",
        "title": "Submit File for Evaluation",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.file.delete",
        "title": "Delete",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.file.rename",
        "title": "Rename",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.file.open",
        "title": "Open Exercise File",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.sortExercisesByDifficulty",
        "title": "Sort By: Difficulty",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.sortExercisesByName",
        "title": "Sort By: Name",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.sortExercisesByTopic",
        "title": "Sort By: Topic",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.sortExercisesByStatus",
        "title": "Sort By: Status",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.clearFilters",
        "title": "Clear All Filters",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.showCompletedExercises",
        "title": "Show Completed Exercises",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.showUncompletedExercises",
        "title": "Show Uncompleted Exercises",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.showDownloadedExercises",
        "title": "Show Downloaded Exercises",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.showJoinedTracks",
        "title": "Show Joined Tracks",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.view.tracks.filterExercisesByTopic",
        "title": "Filter Exercises By Topic",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.track.toggleFocus",
        "title": "Toggle Track Focus",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.track.openAsFolder",
        "title": "Open as Folder",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.track.downloadAllExercises",
        "title": "Download All Exercises",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.global.clearStorageData",
        "title": "Clear Storage Data",
        "category": "Exercism",
        "enablement": "view == 'exercism.view.tracks'"
      },
      {
        "command": "exercism.global.exportUserData",
        "title": "Export User Data",
        "category": "Exercism"
      },
      {
        "command": "exercism.global.importUserData",
        "title": "Import User Data",
        "category": "Exercism"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "exercism.view.tracks.showJoinedTracks",
          "when": "view == 'exercism.view.tracks'",
          "group": "1@2"
        },
        {
          "command": "exercism.view.tracks.showCompletedExercises",
          "when": "view == 'exercism.view.tracks'",
          "group": "1@3"
        },
        {
          "command": "exercism.view.tracks.showUncompletedExercises",
          "when": "view == 'exercism.view.tracks'",
          "group": "1@4"
        },
        {
          "command": "exercism.view.tracks.showDownloadedExercises",
          "when": "view == 'exercism.view.tracks'",
          "group": "1@5"
        },
        {
          "command": "exercism.view.tracks.sortExercisesByDifficulty",
          "when": "view == 'exercism.view.tracks'",
          "group": "2@1"
        },
        {
          "command": "exercism.view.tracks.sortExercisesByName",
          "when": "view == 'exercism.view.tracks'",
          "group": "2@2"
        },
        {
          "command": "exercism.view.tracks.sortExercisesByStatus",
          "when": "view == 'exercism.view.tracks'",
          "group": "2@3"
        },
        {
          "command": "exercism.view.tracks.sortExercisesByTopic",
          "when": "view == 'exercism.view.tracks'",
          "group": "2@4"
        },
        {
          "command": "exercism.global.clearStorageData",
          "when": "view == 'exercism.view.tracks'",
          "group": "3@1"
        },
        {
          "command": "exercism.view.tracks.clearFilters",
          "when": "view == 'exercism.view.tracks'",
          "group": "3@2"
        }
      ],
      "view/item/context": [
        {
          "command": "exercism.exercise.createNewFile",
          "when": "view == 'exercism.view.tracks' && (viewItem == exerciseTreeNode || viewItem == exerciseTreeNode:directory)",
          "group": "1@1"
        },
        {
          "command": "exercism.exercise.openStart",
          "when": "view == 'exercism.view.tracks' && viewItem == exerciseTreeNode",
          "group": "1@2"
        },
        {
          "command": "exercism.exercise.preview",
          "when": "view == 'exercism.view.tracks' && viewItem == exerciseTreeNode",
          "group": "1@3"
        },
        {
          "command": "exercism.exercise.download",
          "when": "view == 'exercism.view.tracks' && viewItem == exerciseTreeNode",
          "group": "2@1"
        },
        {
          "command": "exercism.exercise.complete",
          "when": "view == 'exercism.view.tracks' && viewItem == exerciseTreeNode",
          "group": "2@2"
        },
        {
          "command": "exercism.file.submit",
          "when": "view == 'exercism.view.tracks' && viewItem == fileTreeNode:file",
          "group": "1@1"
        },
        {
          "command": "exercism.file.rename",
          "when": "view == 'exercism.view.tracks' && viewItem =~ /fileTreeNode/",
          "group": "1@2"
        },
        {
          "command": "exercism.file.delete",
          "when": "view == 'exercism.view.tracks' && viewItem =~ /fileTreeNode/",
          "group": "1@3"
        },
        {
          "command": "exercism.track.toggleFocus",
          "when": "view == 'exercism.view.tracks' && viewItem == trackTreeNode",
          "group": "1@1"
        },
        {
          "command": "exercism.track.openAsFolder",
          "when": "view == 'exercism.view.tracks' && viewItem == trackTreeNode",
          "group": "1@2"
        },
        {
          "command": "exercism.track.downloadAllExercises",
          "when": "view == 'exercism.view.tracks' && viewItem == trackTreeNode",
          "group": "1@3"
        }
      ]
    },
    "configuration": [
      {
        "title": "Exercism",
        "properties": {
          "exercism.openStartGlob": {
            "type": "string",
            "default": "**/!(README).{js,go,md,rs,vim,cc,cpp,c,coffee,cs,swift,ts,h,java,lua,rb,sh,vb,php,py}",
            "description": "The file glob to use when opening files for `Open and Start`.",
            "scope": "window"
          },
          "exercism.logging": {
            "type": "string",
            "default": "silent",
            "enum": [
              "silent",
              "error",
              "debug"
            ],
            "enumDescriptions": [
              "Disable all logging.",
              "Only log errors.",
              "Enable all logging."
            ],
            "description": "Specifies the verbosity of the logger. By default all logging is disabled.",
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "pretest": "tsc -p . && tsc -p webview",
    "test": "node ./out/test",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "build-debug": "tsc -w -p . && tsc -w -p webview",
    "profile": "webpack --profile --json > ./out/stats.json && webpack-bundle-analyzer -p 1515 ./out/stats.json",
    "vscode:prepublish": "npm run build-prod"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/fs-extra": "^7.0.0",
    "@types/marked": "^0.6.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.2.0",
    "@types/temp": "^0.8.34",
    "@types/vscode": "^1.35.0",
    "css-loader": "^2.1.1",
    "mocha": "^6.1.4",
    "style-loader": "^0.23.1",
    "temp": "^0.9.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "fast-glob": "^3.0.2",
    "fs-extra": "^8.0.1",
    "helpful-decorators": "^2.0.4",
    "marked": "^0.6.2",
    "preact": "^8.4.2",
    "redux-zero": "^5.0.4"
  }
}
